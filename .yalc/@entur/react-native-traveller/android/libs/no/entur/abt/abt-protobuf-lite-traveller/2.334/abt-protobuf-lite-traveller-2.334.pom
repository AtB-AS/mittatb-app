            <plugin>
                <groupId>org.xolstice.maven.plugins</groupId>
                <artifactId>protobuf-maven-plugin</artifactId>
                <version>${protobuf-maven-plugin.version}</version>
                <executions>
                    <execution>
                        <id>compile-api-grpc</id>
                        <goals>
                            <goal>compile-custom</goal>
                        </goals>
                        <configuration>
                            <protocArtifact>com.google.protobuf:protoc:${proto.version}:exe:${os.detected.classifier}</protocArtifact>
                            <pluginId>grpc-java</pluginId>
                            <protoSourceRoot>${project.build.directory}/proto</protoSourceRoot>
                            <attachProtoSources>false</attachProtoSources>
                            <pluginParameter>lite</pluginParameter>
                            <pluginArtifact>io.grpc:protoc-gen-grpc-java:${grpc.version}:exe:${os.detected.classifier}</pluginArtifact>
                        </configuration>
                    </execution>
                    <execution>
                        <id>compile-api-lite</id>
                        <goals>
                            <goal>compile</goal>
                        </goals>
                        <configuration>
                            <javaOptions>lite</javaOptions>
                            <protocArtifact>com.google.protobuf:protoc:${proto.version}:exe:${os.detected.classifier}</protocArtifact>
                            <protoSourceRoot>${project.build.directory}/proto</protoSourceRoot>
                            <attachProtoSources>false</attachProtoSources>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
