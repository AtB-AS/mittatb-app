{
  "name": "@entur/react-native-traveller",
  "version": "1.0.0",
  "description": "React Native wrapper for account-based ticketing",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android/libs",
    "android/src",
    "android/build.gradle",
    "android/gradle.properties",
    "ios/EnturTraveller.xcodeproj/project.pbxproj",
    "ios/EnturTraveller-Bridging-Header.h",
    "ios/EnturTraveller.m",
    "ios/EnturTraveller.swift",
    "cpp",
    "entur-react-native-traveller.podspec",
    "AbtMobile.xcframework",
    "!lib/typescript/example",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!.vscode",
    "!.circleci",
    "!example"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "prepublishOnly": "sh scripts/prepublishOnly.sh"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/entur/",
  "author": "Entur AS <feedback@entur.org> (https://github.com/entur/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/entur//issues"
  },
  "homepage": "https://github.com/entur/#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "^1.15.9",
    "@react-native-community/netinfo": "^6.1.0",
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/src/__tests__/helpers",
      "<rootDir>/lib/"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "yalcSig": "65e27077a0a78785de1d862afc6a58ed"
}
