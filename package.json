{
  "name": "atb",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --appIdSuffix debug",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "icons": "app-icon generate --platforms=android,ios --rounded none && app-icon generate -i icon.round.png --platforms=android --rounded only",
    "release-draft": "sh tools/release/release-draft.sh",
    "generate-svg-mono-icons": "svgr --config-file .svgrrc.icons.yml -d ./src/assets/svg/icons ./assets/svgs/mono-icons && prettier --write ./src/assets/svg/icons",
    "generate-svg-color": "svgr --config-file .svgrrc.illustrations.yml -d ./src/assets/svg/ ./assets/svgs/color && prettier --write ./src/assets/svg/",
    "generate-svgs": "yarn generate-svg-mono-icons && yarn generate-svg-color"
  },
  "dependencies": {
    "@entur/sdk": "^1.5.0",
    "@react-native-community/async-storage": "^2.0.0-rc.1",
    "@react-native-community/async-storage-backend-legacy": "^2.0.0-rc.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/analytics": "^6.7.1",
    "@react-native-firebase/app": "^6.7.1",
    "@react-native-firebase/remote-config": "^6.7.1",
    "@react-navigation/bottom-tabs": "^5.3.3",
    "@react-navigation/core": "^5.4.0",
    "@react-navigation/native": "^5.2.3",
    "@react-navigation/stack": "^5.2.18",
    "@seznam/compose-react-refs": "^1.0.4",
    "axios": "^0.19.2",
    "bugsnag-react-native": "^2.23.7",
    "date-fns": "^2.14.0",
    "emoji-datasource": "^5.0.1",
    "haversine-distance": "^1.2.1",
    "hex-to-rgba": "^2.0.1",
    "humanize-duration": "^3.23.0",
    "lodash.groupby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.orderby": "^4.6.0",
    "lodash.pickby": "^4.6.0",
    "lottie-ios": "3.1.8",
    "lottie-react-native": "^3.3.2",
    "query-string": "^6.12.1",
    "react": "16.11.0",
    "react-native": "0.62.1",
    "react-native-bootsplash": "^2.2.4",
    "react-native-dash": "^0.0.11",
    "react-native-date-picker": "^2.7.10",
    "react-native-device-info": "^5.6.1",
    "react-native-geolocation-service": "^4.0.1",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-get-random-values": "^1.4.0",
    "react-native-intercom": "^16.0.0",
    "react-native-modalize": "^2.0.4",
    "react-native-permissions": "^2.1.4",
    "react-native-portalize": "^1.0.3",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.7.0",
    "react-native-shared-element": "^0.7.0",
    "react-native-svg": "^12.1.0",
    "react-native-webview": "^9.4.0",
    "react-navigation-shared-element": "^5.0.0-alpha1",
    "search-params": "^3.0.0",
    "string.fromcodepoint": "^1.0.0",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "@svgr/cli": "^5.4.0",
    "@types/humanize-duration": "^3.18.0",
    "@types/jest": "^25.2.1",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.mapvalues": "^4.6.6",
    "@types/lodash.orderby": "^4.6.6",
    "@types/lodash.pickby": "^4.6.6",
    "@types/react-native": "^0.62.5",
    "@types/react-test-renderer": "16.9.2",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "app-icon": "^0.13.2",
    "babel-jest": "^25.5.1",
    "conventional-changelog-cli": "^2.0.31",
    "detox": "^16.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-react-native": "^3.8.1",
    "jest": "^25.5.4",
    "metro-react-native-babel-preset": "^0.59.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.0.5",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/AtB.app",
        "build": "xcodebuild -workspace ios/atb.xcworkspace -scheme atb -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 7 Plus"
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/atb.app",
        "build": "set -o pipefail && export CODE_SIGNING_REQUIRED=NO && export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -workspace ios/atb.xcworkspace -UseNewBuildSystem=NO -scheme atb -configuration Release -sdk iphonesimulator -derivedDataPath ios/build | xcpretty",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      }
    },
    "test-runner": "jest"
  }
}
