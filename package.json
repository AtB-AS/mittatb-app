{
  "name": "atb",
  "version": "0.0.1",
  "private": true,
  "license": "EUPL-1.2",
  "scripts": {
    "android": "react-native run-android --appIdSuffix debug",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "icons": "app-icon generate --platforms=android,ios --rounded none && app-icon generate -i icon.round.png --platforms=android --rounded only",
    "release-draft": "sh tools/release/release-draft.sh",
    "generate-svg-mono-icons": "svgr --config-file .svgrrc.mono-icons.yml -d ./src/assets/svg/icons ./assets/svgs/mono-icons && prettier --write ./src/assets/svg/icons",
    "generate-svg-color": "svgr --config-file .svgrrc.color.yml -d ./src/assets/svg/ ./assets/svgs/color && prettier --write ./src/assets/svg/",
    "generate-svgs": "yarn generate-svg-mono-icons && yarn generate-svg-color",
    "build:ios": "fastlane ios build",
    "updatedevices:ios": "fastlane ios update_devices",
    "appcenter:ios": "fastlane ios appcenter",
    "cert:ios": "fastlane ios get_cert",
    "build:android": "fastlane android build",
    "appcenter:android": "fastlane android appcenter"
  },
  "dependencies": {
    "@bugsnag/react-native": "^7.5.0",
    "@entur/sdk": "^3.1.0",
    "@leile/lobo-t": "^1.0.5",
    "@mapbox/polyline": "^1.1.1",
    "@react-native-community/async-storage": "^2.0.0-rc.1",
    "@react-native-community/async-storage-backend-legacy": "^2.0.0-rc.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/analytics": "^7.6.7",
    "@react-native-firebase/app": "^8.4.5",
    "@react-native-firebase/remote-config": "^9.0.10",
    "@react-native-mapbox-gl/maps": "8.1.0-rc.9",
    "@react-navigation/bottom-tabs": "^5.9.2",
    "@react-navigation/core": "^5.12.5",
    "@react-navigation/material-top-tabs": "^5.3.0",
    "@react-navigation/native": "^5.7.6",
    "@react-navigation/stack": "^5.9.3",
    "@seznam/compose-react-refs": "^1.0.4",
    "@types/mapbox__polyline": "^1.0.1",
    "axios": "^0.20.0",
    "axios-retry": "^3.1.9",
    "date-fns": "^2.16.1",
    "emoji-datasource": "^6.0.0",
    "haversine-distance": "^1.2.1",
    "hex-to-rgba": "^2.0.1",
    "humanize-duration": "^3.24.0",
    "lodash.groupby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.orderby": "^4.6.0",
    "lodash.pickby": "^4.6.0",
    "lottie-ios": "3.1.8",
    "lottie-react-native": "^3.5.0",
    "query-string": "^6.13.5",
    "react": "16.14.0",
    "react-native": "0.63.3",
    "react-native-bootsplash": "^2.2.6",
    "react-native-dash": "^0.0.11",
    "react-native-date-picker": "^3.2.4",
    "react-native-device-info": "^6.2.0",
    "react-native-geolocation-service": "^5.0.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-get-random-values": "^1.5.0",
    "react-native-intercom": "^18.0.0",
    "react-native-localize": "^1.4.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modalize": "^2.0.6",
    "react-native-permissions": "^2.2.2",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^2.15.2",
    "react-native-webview": "^11.0.0",
    "recyclerlistview": "^3.0.0",
    "search-params": "^3.0.0",
    "string.fromcodepoint": "^1.0.0",
    "use-reducer-with-side-effects": "^2.2.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/runtime": "^7.12.0",
    "@svgr/cli": "^5.4.0",
    "@types/geojson": "^7946.0.7",
    "@types/humanize-duration": "^3.18.1",
    "@types/jest": "^26.0.14",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.mapvalues": "^4.6.6",
    "@types/lodash.orderby": "^4.6.6",
    "@types/lodash.pickby": "^4.6.6",
    "@types/react-native": "^0.63.25",
    "@types/react-test-renderer": "16.9.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "app-icon": "^0.13.2",
    "babel-jest": "^26.5.2",
    "conventional-changelog-cli": "^2.1.0",
    "detox": "^17.9.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-react-native": "3.8.1",
    "jest": "^26.5.3",
    "metro-react-native-babel-preset": "^0.63.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.1.2",
    "react-native-dotenv": "^2.4.1",
    "react-test-renderer": "16.14.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/AtB.app",
        "build": "xcodebuild -workspace ios/atb.xcworkspace -scheme atb -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 7 Plus"
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/atb.app",
        "build": "set -o pipefail && export CODE_SIGNING_REQUIRED=NO && export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -workspace ios/atb.xcworkspace -UseNewBuildSystem=NO -scheme atb -configuration Release -sdk iphonesimulator -derivedDataPath ios/build | xcpretty",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      }
    },
    "test-runner": "jest"
  }
}
