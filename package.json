{
  "name": "atb",
  "version": "1.6.0",
  "private": true,
  "license": "EUPL-1.2",
  "scripts": {
    "android": "react-native run-android --appIdSuffix debug",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "icons": "app-icon generate --platforms=android,ios --rounded none && app-icon generate -i icon.round.png --platforms=android --rounded only",
    "release-draft": "sh tools/release/release-draft.sh",
    "generate-svg-mono-icons": "svgr --config-file .svgrrc.mono-icons.yml -d ./src/assets/svg/icons ./assets/svgs/mono-icons && prettier --write ./src/assets/svg/icons",
    "generate-svg-color": "svgr --config-file .svgrrc.color.yml -d ./src/assets/svg/ ./assets/svgs/color && prettier --write ./src/assets/svg/",
    "generate-svgs": "yarn generate-svg-mono-icons && yarn generate-svg-color",
    "update-device-profiles": "bundle exec fastlane ios update_devices"
  },
  "dependencies": {
    "@atb-as/theme": "^1.2.0",
    "@bugsnag/react-native": "^7.6.1",
    "@entur/sdk": "^3.4.0",
    "@leile/lobo-t": "^1.0.5",
    "@mapbox/polyline": "^1.1.1",
    "@react-native-community/async-storage": "^2.0.0-rc.1",
    "@react-native-community/async-storage-backend-legacy": "^2.0.0-rc.1",
    "@react-native-community/datetimepicker": "^3.0.9",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/analytics": "10.1.0",
    "@react-native-firebase/app": "10.1.0",
    "@react-native-firebase/auth": "10.1.0",
    "@react-native-firebase/firestore": "10.1.0",
    "@react-native-firebase/messaging": "10.1.0",
    "@react-native-firebase/remote-config": "10.1.0",
    "@react-native-mapbox-gl/maps": "8.1.0",
    "@react-navigation/bottom-tabs": "^5.11.7",
    "@react-navigation/core": "^5.15.1",
    "@react-navigation/material-top-tabs": "^5.3.13",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.2",
    "@seznam/compose-react-refs": "^1.0.5",
    "@turf/boolean-point-in-polygon": "^6.3.0",
    "@turf/centroid": "^6.3.0",
    "@types/lodash.sortby": "^4.7.6",
    "@types/mapbox__polyline": "^1.0.2",
    "@types/qrcode": "^1.4.0",
    "axios": "^0.21.1",
    "axios-better-stacktrace": "^1.2.0",
    "axios-retry": "^3.1.9",
    "date-fns": "^2.17.0",
    "emoji-datasource": "^6.0.0",
    "haversine-distance": "^1.2.1",
    "hex-to-rgba": "^2.0.1",
    "humanize-duration": "^3.25.1",
    "lodash.groupby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.orderby": "^4.6.0",
    "lodash.pickby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "qrcode": "^1.4.4",
    "query-string": "^6.13.8",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-bootsplash": "^3.1.3",
    "react-native-dash": "^0.0.11",
    "react-native-device-info": "^8.0.1",
    "react-native-geolocation-service": "^5.2.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-get-random-values": "^1.5.1",
    "react-native-intercom": "^18.0.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^2.0.1",
    "react-native-modalize": "^2.0.8",
    "react-native-permissions": "^3.0.0",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.17.1",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^2.15.2",
    "react-native-webview": "^11.2.1",
    "recyclerlistview": "^3.0.5",
    "search-params": "^3.0.0",
    "string.fromcodepoint": "^1.0.0",
    "use-reducer-with-side-effects": "^2.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@svgr/cli": "^5.5.0",
    "@types/geojson": "^7946.0.7",
    "@types/humanize-duration": "^3.18.1",
    "@types/jest": "^26.0.20",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.mapvalues": "^4.6.6",
    "@types/lodash.orderby": "^4.6.6",
    "@types/lodash.pickby": "^4.6.6",
    "@types/react-native": "^0.63.48",
    "@types/react-test-renderer": "^16.9.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "app-icon": "^0.13.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "detox": "^18.2.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "3.8.1",
    "eslint-plugin-rulesdir": "^0.1.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.2.1",
    "react-native-dotenv": "^2.5.1",
    "react-test-renderer": "16.13.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/AtB.app",
        "build": "xcodebuild -workspace ios/atb.xcworkspace -scheme atb -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 7 Plus"
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/atb.app",
        "build": "set -o pipefail && export CODE_SIGNING_REQUIRED=NO && export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -workspace ios/atb.xcworkspace -UseNewBuildSystem=NO -scheme atb -configuration Release -sdk iphonesimulator -derivedDataPath ios/build | xcpretty",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      }
    },
    "test-runner": "jest"
  }
}
